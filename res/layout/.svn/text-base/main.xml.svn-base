<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/OverAll" android:layout_height="fill_parent"
	android:layout_width="fill_parent" android:orientation="vertical">
	
	<HorizontalScrollView android:id="@+id/horizScroll"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_weight="19">
		
		<LinearLayout android:id="@+id/layout1"
			android:orientation="horizontal" android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			
		</LinearLayout>
		
	</HorizontalScrollView>
	<LinearLayout android:id="@+id/TopBar" android:layout_weight="1" android:layout_gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="50dp">
		<Button android:id="@+id/backToStartButton" android:layout_width="wrap_content" android:layout_height="match_parent" android:paddingBottom="10dp" android:paddingTop="10dp" android:layout_gravity="left" android:text="@string/sources" android:minHeight="50dp"></Button>
		<Button android:id="@+id/Settings" android:layout_width="wrap_content" android:layout_height="match_parent" android:layout_gravity="right" android:text="@string/manage" android:minHeight="50dp"></Button>
	</LinearLayout>
	
</LinearLayout>